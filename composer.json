{
    "name": "drewlabs/net",
    "description": "Networking utilities libraries",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Drewlabs\\Net\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Net\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}